                                                          maximumLength="#{bindings.newPassword.hints.precision}"
                                                          shortDesc="#{bindings.newPassword.hints.tooltip}" id="it20"
                                                          secret="true" binding="#{shuttle_group.newPasswd}">
                                                <f:validator binding="#{bindings.newPassword.validator}"/>
