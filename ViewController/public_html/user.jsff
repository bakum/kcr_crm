                                        <af:toolbar id="t11">
                                            <af:button actionListener="#{bindings.Execute.execute}" text="Обновить"
                                                       disabled="#{!bindings.Execute.enabled}" id="b17"/>
                                        </af:toolbar>
                                    </f:facet>
                                    <af:table value="#{bindings.onlineUserList.collectionModel}" var="row"
                                              rows="#{bindings.onlineUserList.rangeSize}"
                                              emptyText="#{bindings.onlineUserList.viewable ? 'No data to display.' : 'Access Denied.'}"
                                              rowBandingInterval="0"
                                              selectedRowKeys="#{bindings.onlineUserList.collectionModel.selectedRow}"
                                              selectionListener="#{bindings.onlineUserList.collectionModel.makeCurrent}"
                                              rowSelection="single" fetchSize="#{bindings.onlineUserList.rangeSize}"
                                              id="t10" partialTriggers="::b17">
                                        <af:column sortProperty="#{bindings.onlineUserList.hints.id.name}"
                                                   sortable="true"
                                                   headerText="GUID" id="c23"
                                                   width="270">
                                            <af:outputText value="#{row.id}"
                                                           shortDesc="#{bindings.onlineUserList.hints.id.tooltip}"
                                                           id="ot1"
                                                           inlineStyle="#{securityContext.userName == row.login ? 'font-weight:bold;' : ''}"/>
                                        </af:column>
                                        <af:column sortProperty="#{bindings.onlineUserList.hints.login.name}"
                                                   sortable="true" headerText="Логин" id="c24" width="150">
                                            <af:outputText value="#{row.login}"
                                                           shortDesc="#{bindings.onlineUserList.hints.login.tooltip}"
                                                           id="ot2"
                                                           inlineStyle="#{securityContext.userName == row.login ? 'font-weight:bold;' : ''}"/>
                                        </af:column>
                                        <af:column sortProperty="#{bindings.onlineUserList.hints.logDate.name}"
                                                   sortable="true" headerText="Дата входа" id="c25" width="150">
                                            <af:outputText value="#{row.logDate}"
                                                           shortDesc="#{bindings.onlineUserList.hints.logDate.tooltip}"
                                                           id="ot3"
                                                           inlineStyle="#{securityContext.userName == row.login ? 'font-weight:bold;' : ''}">
                                                <af:convertDateTime pattern="dd-MMM-yyy HH:mm:ss"/>
                                            </af:outputText>
                                        </af:column>
                                        <af:column headerText="IP сервера"
                                                   id="c26" width="200">
                                            <af:outputText value="#{row.ipAdress}"
                                                           shortDesc="#{bindings.onlineUserList.hints.ipAdress.tooltip}"
                                                           id="ot4"
                                                           inlineStyle="#{securityContext.userName == row.login ? 'font-weight:bold;' : ''}"/>
                                        </af:column>
                                        <af:column headerText="#{bindings.onlineUserList.hints.ipAdressLocal.label}"
                                                   id="c27" width="200">
                                            <af:outputText value="#{row.ipAdressLocal}"
                                                           shortDesc="#{bindings.onlineUserList.hints.ipAdressLocal.tooltip}"
                                                           id="ot5"
                                                           inlineStyle="#{securityContext.userName == row.login ? 'font-weight:bold;' : ''}"/>
                                        </af:column>
                                        <af:column headerText="#{bindings.onlineUserList.hints.ipAdressNAT.label}"
                                                   id="c28" width="200">
                                            <af:outputText value="#{row.ipAdressNAT}"
                                                           shortDesc="#{bindings.onlineUserList.hints.ipAdressNAT.tooltip}"
                                                           id="ot6"
                                                           inlineStyle="#{securityContext.userName == row.login ? 'font-weight:bold;' : ''}"/>
                                        </af:column>
                                    </af:table>
                                    <f:facet name="statusbar"/>
                                </af:panelCollection>
                            </af:showDetailItem>
                        </af:panelTabbed>
                    </f:facet>
                    <f:facet name="top"/>
                </af:decorativeBox>
            </af:gridCell>
        </af:gridRow>
    </af:panelGridLayout>
