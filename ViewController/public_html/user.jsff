                                            <af:panelGroupLayout id="pgl3" layout="horizontal" halign="right">
                                                <af:toolbar id="t8">
                                                    <af:button text="Ok" id="bNewKontrag" immediate="true"
                                                               actionListener="#{shuttle_group.onSaveKontrag}"/>
                                                    <af:button text="Отмена" id="b18" immediate="true"
                                                               actionListener="#{shuttle_group.onCancelKontrag}"/>
                                                </af:toolbar>
                                            </af:panelGroupLayout>
                                        </f:facet>
                                        <af:inputText label="Имя" id="it25" required="true"
                                                      value="#{bindings.KontragName1.inputValue}" columns="30"
                                                      binding="#{shuttle_group.kontragName}" autoSubmit="true"/>
                                    </af:dialog>
                                </af:popup>
                            </af:showDetailItem>
                            <af:showDetailItem text="Активные пользователи" id="sdi7" stretchChildren="first">
