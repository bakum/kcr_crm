                                            <af:inputText value="#{bindings.username1.inputValue}"
                                                          label="Логин"
                                                          required="#{bindings.username1.hints.mandatory}"
                                                          columns="#{bindings.username1.hints.displayWidth}"
                                                          maximumLength="#{bindings.username1.hints.precision}"
                                                          shortDesc="#{bindings.username1.hints.tooltip}" id="it21"
                                                          showRequired="true" binding="#{shuttle_group.login}">
                                                <f:validator binding="#{bindings.username1.validator}"/>
                                            </af:inputText>
                                            <af:inputText value="#{bindings.psw.inputValue}"
                                                          label="Пароль"
                                                          required="#{bindings.psw.hints.mandatory}"
                                                          columns="#{bindings.psw.hints.displayWidth}"
                                                          maximumLength="#{bindings.psw.hints.precision}"
                                                          shortDesc="#{bindings.psw.hints.tooltip}" id="it22"
                                                          secret="true" showRequired="true"
                                                          binding="#{shuttle_group.passwd}">
                                                <f:validator binding="#{bindings.psw.validator}"/>
                                            </af:inputText>
                                            <af:inputText value="#{bindings.desc.inputValue}"
                                                          label="Примечание"
                                                          required="#{bindings.desc.hints.mandatory}"
                                                          columns="#{bindings.desc.hints.displayWidth}"
                                                          maximumLength="#{bindings.desc.hints.precision}"
                                                          shortDesc="#{bindings.desc.hints.tooltip}" id="it23" rows="2"
                                                          binding="#{shuttle_group.desc}">
                                                <f:validator binding="#{bindings.desc.validator}"/>
                                            </af:inputText>
                                            <af:button actionListener="#{bindings.addUserToWls.execute}"
                                                       text="addUserToWls" disabled="#{!bindings.addUserToWls.enabled}"
                                                       id="b11" rendered="false"/>
                                        </af:panelFormLayout>
                                    </af:dialog>
                                </af:popup>
                                <af:popup childCreation="deferred" autoCancel="disabled" id="pZamer"
                                          contentDelivery="lazyUncached"
                                          popupFetchListener="#{shuttle_group.onPopupCreateZamer}"
                                          binding="#{shuttle_group.newZamerPopup}" resetEditableValues="whenCanceled">
                                    <af:dialog id="d6" title="Новый контрагент для замеров"
                                               dialogListener="#{shuttle_group.onNewZamerDialogListener}"
                                               closeIconVisible="false" type="none">
                                        <f:facet name="buttonBar">
                                            <af:panelGroupLayout id="pgl4" layout="horizontal" halign="right">
                                                <af:toolbar id="t9">
                                                    <af:button text="Ok" id="bNewZamer" immediate="true"
                                                               actionListener="#{shuttle_group.onSaveZamer}"/>
                                                    <af:button text="Отмена" id="b19" immediate="true"
                                                               actionListener="#{shuttle_group.onCancelZamer}"/>
                                                </af:toolbar>
                                            </af:panelGroupLayout>
                                        </f:facet>
                                        <af:inputText label="Наименование" id="it24"
                                                      value="#{bindings.KonName1.inputValue}" columns="30"
                                                      binding="#{shuttle_group.konName}" required="true"
                                                      autoSubmit="true"/>
                                    </af:dialog>
                                    <af:resetListener type="popupCanceled"/>
                                </af:popup>
                                <af:popup childCreation="deferred" autoCancel="disabled" id="pKontrag"
                                          contentDelivery="lazyUncached"
                                          popupFetchListener="#{shuttle_group.onPopupCreateKontrag}"
                                          binding="#{shuttle_group.newKontragPopup}">
                                    <af:dialog id="d7" title="Новый контрагент для расчетов"
                                               dialogListener="#{shuttle_group.onNewKontragDialogListener}" type="none"
                                               closeIconVisible="false">
