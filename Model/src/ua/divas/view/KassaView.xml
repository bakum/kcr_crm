       Kassa.IS_GROUP, 
       Kassa.PARENT_ID, 
       Kassa.CURR_ID, 
       Kassa.DIVISION_ID, 
       Kassa.FIRMA_ID, 
       Kassa.DELETED, 
       Kassa.VERSION, 
       Kassa.PREDEFINED,
       case
        when (ID not in (select kassa_id from ballans_by_kassa where ballans !=0)) then 1
        else 0
       end Nullable,
       (select 0 from dual) k_editable
FROM  KASSA Kassa
where deleted = 0 and
upper(Kassa.DIVISION_ID) in (select upper(N) from table(cast(get_divisions(:u_name) as divisionTable)))]]></SQLQuery>
