    <SQLQuery><![CDATA[SELECT 
    Orders.ID, 
    Orders.DAT, 
    Orders.NUM, 
    Orders.KONTRAG_ID, 
    (SELECT 
        FULLNAME 
    FROM 
        KONTRAGENTS 
    WHERE 
        ID = KONTRAG_ID) KontragName, 
    Orders.CURR_ID, 
    Orders.KASSA_ID, 
    Orders.DIVISION_ID, 
    Orders.DIVISION_DEST,
    Orders.USER_ID, 
    Orders.KURS, 
    Orders.KRATNOST, 
    Orders.DISCRIPTION, 
    Orders.DELETED, 
    Orders.VERSION, 
    Orders.POSTED, 
    Orders.FIRM_ID, 
    Orders.ACTIVITIES_ID, 
    Orders.STATUS_ID, 
    (SELECT 
        0 
    FROM 
        dual) o_editable, 
    Orders.ZAMER_ID, 
    (SELECT 
        FULLNAME 
    FROM 
        KONTRAGENTS 
    WHERE 
        ID = ZAMER_ID) ZamerName, 
    Orders.DAT_COMPLETE DAT_COMPLETE, 
    Orders.DAT_COMPLETE_FACT DAT_COMPLETE_FACT, 
    Orders.DAT_ZAM DAT_ZAM, 
    Orders.AGENT_ID,
    (SELECT 
        FULLNAME 
    FROM 
        KONTRAGENTS 
    WHERE 
        ID = AGENT_ID) Agent_Name,
    Orders.SUMM_PLAN SUMM_PLAN,
    'Заказ' ObjName,
    UTILITY.enable_edit(Orders.ID,'orders') enbl,
    (select kontrag_id from user_settings where user_id = Orders.USER_ID) Manager_ID
FROM 
    ORDERS Orders 
WHERE 
upper(Orders.USER_ID) in (select upper(N) from table(cast(get_users(:u_name) as userTable)))]]></SQLQuery>
