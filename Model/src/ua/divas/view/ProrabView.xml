<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="ProrabView"
  Version="12.1.2.66.68"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  ComponentClass="ua.divas.view.ProrabViewImpl"
  RowClass="ua.divas.view.ProrabViewRowImpl">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
    <Attr Name="_codeGenFlag2" Value="Access|Coll|VarAccess"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="ua.divas.view.ProrabView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="u_name"
    Kind="where"
    Type="java.lang.String">
    <TransientExpression><![CDATA[viewObject.getSessionUser()]]></TransientExpression>
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery><![CDATA[SELECT Kontragents.DELETED,  
       Kontragents.FULLNAME,  
       Kontragents.ID,  
       Kontragents.INN,  
       Kontragents.IS_BUYER,  
       Kontragents.IS_GROUP,  
       Kontragents.IS_MEASURER,  
       Kontragents.IS_SUPPLIER,  
       Kontragents.KPP,  
       Kontragents.NAMEFULL,  
       Kontragents.OKPO,  
       Kontragents.PARENT_ID,  
       Kontragents.PREDEFINED, 
       (select 0 from dual) editable, 
       Kontragents.UR_FIZ,  
       Kontragents.USER_ID,  
       Kontragents.VERSION 
FROM  KONTRAGENTS Kontragents 
where Kontragents.IS_SUPPLIER = 1 and 
upper(Kontragents.USER_ID) in (select upper(N) from table(cast(get_users(:u_name) as userTable))) 
order by Kontragents.FULLNAME]]></SQLQuery>
  <EntityUsage
    Name="Kontragents"
    Entity="ua.divas.model.Kontragents"/>
  <ViewAttribute
    Name="Deleted"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Deleted"
    EntityUsage="Kontragents"
    AliasName="DELETED"
    IsQueriable="false"/>
  <ViewAttribute
    Name="Fullname"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Fullname"
    EntityUsage="Kontragents"
    AliasName="FULLNAME"/>
  <ViewAttribute
    Name="Id"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Id"
    EntityUsage="Kontragents"
    AliasName="ID"
    IsQueriable="false"/>
  <ViewAttribute
    Name="Inn"
    PrecisionRule="true"
    EntityAttrName="Inn"
    EntityUsage="Kontragents"
    AliasName="INN"
    IsQueriable="false"/>
  <ViewAttribute
    Name="IsBuyer"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="IsBuyer"
    EntityUsage="Kontragents"
    AliasName="IS_BUYER"
    IsQueriable="false"/>
  <ViewAttribute
    Name="IsGroup"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="IsGroup"
    EntityUsage="Kontragents"
    AliasName="IS_GROUP"
    IsQueriable="false"/>
  <ViewAttribute
    Name="IsMeasurer"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="IsMeasurer"
    EntityUsage="Kontragents"
    AliasName="IS_MEASURER"
    IsQueriable="false"/>
  <ViewAttribute
    Name="IsSupplier"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="IsSupplier"
    EntityUsage="Kontragents"
    AliasName="IS_SUPPLIER"
    DefaultValue="1"
    IsQueriable="false"/>
  <ViewAttribute
    Name="Kpp"
    PrecisionRule="true"
    EntityAttrName="Kpp"
    EntityUsage="Kontragents"
    AliasName="KPP"
    IsQueriable="false"/>
  <ViewAttribute
    Name="Namefull"
    PrecisionRule="true"
    EntityAttrName="Namefull"
    EntityUsage="Kontragents"
    AliasName="NAMEFULL"
    IsQueriable="false"/>
  <ViewAttribute
    Name="Okpo"
    PrecisionRule="true"
    EntityAttrName="Okpo"
    EntityUsage="Kontragents"
    AliasName="OKPO"
    IsQueriable="false"/>
  <ViewAttribute
    Name="ParentId"
    PrecisionRule="true"
    EntityAttrName="ParentId"
    EntityUsage="Kontragents"
    AliasName="PARENT_ID"
    IsQueriable="false"/>
  <ViewAttribute
    Name="Predefined"
    PrecisionRule="true"
    EntityAttrName="Predefined"
    EntityUsage="Kontragents"
    AliasName="PREDEFINED"
    IsQueriable="false"/>
  <ViewAttribute
    Name="Editable"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="EDITABLE"
    Expression="EDITABLE"
    SQLType="NUMERIC"
    IsQueriable="false"/>
  <ViewAttribute
    Name="UrFiz"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="UrFiz"
    EntityUsage="Kontragents"
    AliasName="UR_FIZ"
    IsQueriable="false"/>
  <ViewAttribute
    Name="UserId"
    PrecisionRule="true"
    EntityAttrName="UserId"
    EntityUsage="Kontragents"
    AliasName="USER_ID"
    IsQueriable="false">
    <TransientExpression><![CDATA[viewObject.getSessionUserId()]]></TransientExpression>
  </ViewAttribute>
  <ViewAttribute
    Name="Version"
    PrecisionRule="true"
    EntityAttrName="Version"
    EntityUsage="Kontragents"
    AliasName="VERSION"
    IsQueriable="false"/>
  <ViewAttribute
    Name="TotalSumm"
    IsUpdateable="false"
    IsSelected="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="VIEW_ATTR"
    SQLType="NUMERIC">
    <RecalcCondition><![CDATA[true]]></RecalcCondition>
    <TransientExpression><![CDATA[ProrabReport.sum("Summ != null ? Summ : 0")]]></TransientExpression>
  </ViewAttribute>
  <ViewAttribute
    Name="TotalOrderSumm"
    IsUpdateable="false"
    IsSelected="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="VIEW_ATTR"
    SQLType="NUMERIC">
    <RecalcCondition><![CDATA[true]]></RecalcCondition>
    <TransientExpression><![CDATA[ProrabReport.sum("SummOrder != null ? SummOrder : 0")]]></TransientExpression>
  </ViewAttribute>
  <ViewAttribute
    Name="DebtSupplier"
    IsUpdateable="false"
    IsSelected="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="VIEW_ATTR"
    SQLType="NUMERIC">
    <RecalcCondition><![CDATA[true]]></RecalcCondition>
    <TransientExpression><![CDATA[VwSupplierMoves.sum("SumSumm != null ? (-1)*SumSumm : 0")]]></TransientExpression>
  </ViewAttribute>
  <ViewLinkAccessor
    Name="ProrabReport"
    ViewLink="ua.divas.view.ProrabProrabRepFkLink"
    Type="oracle.jbo.RowIterator"
    IsUpdateable="false"/>
  <ViewLinkAccessor
    Name="VwSupplierMoves"
    ViewLink="ua.divas.view.ProrabSupplierFkLink"
    Type="oracle.jbo.RowIterator"
    IsUpdateable="false"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="ua.divas.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
